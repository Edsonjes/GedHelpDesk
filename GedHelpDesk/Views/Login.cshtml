@model GedHelpDesk.Models.Login
@{
    ViewData["Title"] = "Login";

}

<div class="container">
    <div class="row">
        <div class="col-md-4 col-md-offset-4">
            <div class="">
                <div class="panel-heading">
                    <h3 class="panel-title">Insira seu dados</h3>
                </div>
            </div>
            <div class="panel-body">
                <fieldset>
                    <div class="form-group">
                        <input class="form-control" placeholder="Usuario" nome="Usuario" type="text" asp-for="Usuario" />
                    </div>
                    <div class="form-group">
                        <input class="form-control" placeholder="Password" nome="Password" type="text" id="txtPassword" asp-for="Senha" />
                    </div>
                    <button id="btnAcessar" class="btn btn-lg btn-success btn-block">Entrar</button>
                </fieldset>
            </div>

        </div>
    </div>

</div>

